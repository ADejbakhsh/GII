{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "graph.txt",
  "patterns": [
    {
      "include": "#keywords"
    },
    {
      "include": "#strings"
    },
    {
      "include": "#comment"
    },
    {
      "include": "#entity"
    },
    {
      "include": "#constant"
    },
    {
      "include": "#markup"
    }
  ],
  "repository": {
    "keywords": {
      "name": "keyword.control.gii",
      "patterns": [
        {
          "name": "keyword.control.gii",
          "match": "\\b(GRAPH|END)\\b"
        }
      ]
    },
    "strings": {
      "name": "string.quoted.double.GII",
      "begin": "\"",
      "end": "\"",
      "patterns": [
        {
          "name": "constant.character.escape.GII",
          "match": "\\\\."
        }
      ]
    },
    "comment": {
      "name": "comment.block.GII",
      "begin": "#",
      "end": "$",
      "patterns": [
        {
          "name": "comment.block.GII",
          "match": "#.*$"
        }
      ]
    },
    "entity": {
      "name": "entity.name.function.GII",
      "patterns": [
        {
          "name": "entity.name.function.GII",
          "match": "^GRAPH_[a-zA-Z]{0,}"
        }
      ]
    },
    "constant": {
      "name": "constant.GII",
      "patterns": [
        {
          "name": "constant.language.GII",
          "match": "^INDIC_[a-zA-Z]{0,}"
        }
      ]
    },
    "markup": {
      "name": "markup.heading.gii",
      "patterns": [
        {
          "name": "markup.heading.gii",
          "match": "^INDIC:"
        }
      ]
    }
  },
  "scopeName": "source.txt"
}